<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PackageLicenseUrl>http://www.apache.org/licenses/</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/tspence/okapi-doc</PackageProjectUrl>
    <RepositoryUrl>https://github.com/tspence/okapi-doc</RepositoryUrl>
    <Description>OpenAPI Documentation and Code Generator</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="RazorLight2" Version="2.0.0" />
    <PackageReference Include="System.CodeDom" Version="4.4.0" />
  </ItemGroup>

</Project>
